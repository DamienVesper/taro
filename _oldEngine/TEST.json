{
  "name": "game-server",
  "main": "server.js",
  "version": "1.0.0",
  "scripts": {
    "server": "cross-env ENV=standalone node server/ige.js -g ./src",
    "server:prod": "node server/ige.js -g ./src",
    "server:dev": "cross-env ENV=local node server/ige.js -g ./src",
    "server:remote": "cross-env ENV=standalone-remote node server/ige.js -g ./src",
    "build": "cross-env TEST=true node server/ige.js -deploy ./src -to ../be/assets",
    "build:gs": "cross-env TEST=true node server/ige.js -deploy ./src -to ../../be/assets",
    "debug": "cross-env ENV=standalone node --inspect server/ige.js -g ./src"
  },
  "dependencies": {
    "@clusterws/cws": "^0.15.2",
    "babel-core": "^6.26.3",
    "bad-words": "^3.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "clusterws": "^3.3.1",
    "cookie-parser": "^1.4.4",
    "crash-colliders": "^2.0.2",
    "ejs": "^3.1.5",
    "express": "^4.16.4",
    "jsonwebtoken": "8.5.1",
    "lodash": "^4.17.11",
    "lz-string": "^1.4.4",
    "msgpack-lite": "^0.1.26",
    "node-arguments": "0.0.1",
    "node-schedule": "^1.3.2",
    "public-ip": "^2.5.0",
    "q": "^1.5.1",
    "randomstring": "^1.1.5",
    "rate-limiter-flexible": "^2.1.10",
    "request": "^2.88.0",
    "rollbar": "^2.6.1",
    "sanitizer": "^0.1.3",
    "sha1": "^1.1.1",
    "tick": "0.1.1",
    "validator": "^5.7.0",
    "ws": "^7.1.2",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "uglify-js2": "^2.1.11"
  }
}
